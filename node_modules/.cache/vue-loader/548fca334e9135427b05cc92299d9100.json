,\n  created() {\n    this.$store.dispatch(\"fetchTukangs\");\n  },\n  mounted() {\n    setInterval(() => {\n      this.$store.dispatch(\"fetchTukangs\");\n    }, 5000);\n  },\n  computed: {\n    tukangs() {\n      return this.$store.state.tukangs;\n    }\n  }\n};\n</script>\n\n<style>\n.messenger {\n  cursor: pointer;\n}\n</style>\n"]}]}